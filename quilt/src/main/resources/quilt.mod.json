{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "logic_gates",
    "version": "${version}",
    "metadata": {
      "name": "Logic Gates",
      "description": "This mod aims to add simple logic gates block to use in redstone builds",
      "contributors": {
        "Vrglab": "Author"
      },
      "icon": "logo.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "org.Vrglab.LogicGates.quilt.LogicGatesModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.4"
      },
      {
        "id": "architectury",
        "version": ">=11.1.17"
      }
    ]
  },
  "mixin": [
    "logic_gates.mixins.json"
  ]
}
